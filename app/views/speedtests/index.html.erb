<p id="notice"><%= notice %></p>

<h1>Listing Speedtests</h1>

<table>
  <thead>
    <tr>
      <th>Ssid</th>
      <th>Ping</th>
      <th>Upload</th>
      <th>Download</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @speedtests.each do |speedtest| %>
      <tr>
        <td><%= speedtest.ssid %></td>
        <td><%= speedtest.ping %></td>
        <td><%= speedtest.upload %></td>
        <td><%= speedtest.download %></td>
        <td><%= link_to 'Show', speedtest %></td>
        <td><%= link_to 'Edit', edit_speedtest_path(speedtest) %></td>
        <td><%= link_to 'Destroy', speedtest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Speedtest', new_speedtest_path %>
